<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<fx:root fx:id="panel" alignment="CENTER" maxHeight="28.0" prefHeight="28.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="label" prefHeight="25.0" prefWidth="100.0" text="Label" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets right="8.0" />
            </HBox.margin>
        </Label>
        <TextField fx:id="textField" prefHeight="25.0" prefWidth="200.0" />
    </children>
    <padding>
        <Insets bottom="2.0" left="2.0" right="2.0" top="1.0" />
    </padding>
</fx:root>
